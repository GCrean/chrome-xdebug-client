{
	"manifest_version": 2,
	"minimum_chrome_version": "24",
	"name": "Xdebug",
	"description": "Xdebug Client",
	"version": "1.2",
	"offline_enabled": true,
	"app": {
		"background": {
			"scripts": ["js/background.js"]
		}
	},
	"permissions": [
		"storage",
		{"socket": ["tcp-listen::9000"]}
	],
	"icons": {
		"16": "img/icon_16.png",
		"48": "img/icon_48.png",
		"128": "img/icon_128.png"
	}
}
